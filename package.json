{
  "name": "@vrehor/cypress-workshop",
  "version": "0.0.1",
  "description": "Cypress workshop",
  "private": false,
  "license": "MIT",
  "keywords": [
    "cypress",
    "workshop",
    "typescript"
  ],
  "scripts": {
    "start": "nx serve",
    "test": "nx run-many --target=test --all --skip-nx-cache",
    "test:all": "npm test",
    "test:affected": "nx affected --target=test --uncommitted",
    "e2e:app": "nx clear-cache && nx run app-e2e:e2e --base-url http://localhost:3002",
    "e2e:api": "nx clear-cache && nx run api-e2e:e2e --base-url http://localhost:3333/graphql",
    "e2e:app:watch": "nx run app-e2e:e2e --base-url http://localhost:3002 --watch",
    "e2e:api:watch": "nx run api-e2e:e2e --base-url http://localhost:3333/graphql --watch",
    "lint": "nx run-many --target=lint --all --skip-nx-cache --fix",
    "lint:all": "npm run lint",
    "lint:affected": "nx affected --target=lint --uncommitted",
    "typecheck": "nx run-many --target=typecheck --all --parallel --maxParallel=10 --skip-nx-cache",
    "typecheck:all": "npm run typecheck",
    "typecheck:affected": "nx affected --target=typecheck --uncommitted",
    "format": "prettier --write .",
    "prepare": "husky install .husky",
    "pre-commit": "lint-staged",
    "pre-push": "npm run typecheck && npm run test",
    "codegen": "nx run data-access:generate",
    "generate:nexus": "cross-env NODE_ENV=development dotenv -- ts-node --project ./apps/api/tsconfig.app.json --transpile-only --files ./apps/api/src/config/apollo/schema.ts",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:save": "prisma migrate dev --create-only",
    "prisma:migrate:reset": "prisma migrate reset --force --skip-seed",
    "prisma:generate": "prisma generate",
    "prisma:seed:dev": "ts-node --project ./apps/api/tsconfig.app.json --files ./apps/api/prisma/seed.ts dev",
    "serve:all": "nx run-many --target=serve --projects=api,app",
    "serve:storybook": "npx nx run common-ui:storybook",
    "serve:app": "npx nx run app:serve",
    "serve:api": "npx nx run api:serve",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer ./dist/apps/app/stats.json"
  },
  "prisma": {
    "schema": "apps/api/prisma/schema.prisma"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "16.x.x",
    "npm": "8.x.x"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@apollo/client": "^3.4.16",
    "@graphql-tools/schema": "^8.3.1",
    "@prisma/client": "^4.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.0",
    "@types/express-request-id": "^1.4.3",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.181",
    "@types/node": "^16.11.27",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "antd": "~4.20.0",
    "apollo-server": "^3.4.0",
    "apollo-server-express": "^3.5.0",
    "apollo-upload-client": "^17.0.0",
    "apollo-utilities": "^1.3.4",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.17.0",
    "express-jwt": "^7.7.7",
    "express-request-id": "^1.4.1",
    "graphql": "^15.7.2",
    "graphql-middleware": "^6.1.13",
    "graphql-scalars": "^1.20.1",
